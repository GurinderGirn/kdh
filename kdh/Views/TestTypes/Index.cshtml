@model IEnumerable<kdh.Models.TestType>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/PatientPortal/_PatientPortal.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Category)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TestItem)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MaxReference)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MinReference)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Unit)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Category)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TestItem)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MaxReference)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MinReference)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Unit)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
